<?xml version="1.0" encoding="UTF-8"?>
  	<suite xmlns="http://www.xframium.org/testRegistry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.xframium.org/testRegistry http://www.xframium.org/xsd/pageManagement/testRegistry.xsd">

	<model>
		<page name="Landing_Page" />
		<page name="Login" />
	</model>

	<test active="true" name="TC_StoreProperty_Testing" timed="true" dataDriver="StoreProperty" >

		<step name="URL" type="DEVICE" page="landing_Page" >
			<parameter type="DATA" value="StoreProperty.url" />
		</step>
		
		<step type="BROWSER" name="MAXIMIZE" page="Landing_Page" wait="1000" />
		
		<!-- Login If Required -->
		<step timed="true" name="username.VISIBLE" page="Landing_Page" type="WAIT_FOR" threshold="30000" inverse="true" failureMode="IGNORE" >
			<!-- Enter Email -->
			<step timed="true" name="username" type="SET" page="shopstartwo" threshold="30000">
				<parameter type="STATIC" value="nitesh.timilsina@halo.com"/>  
			</step>	
		
			<!-- Enter Password -->
			<step timed="true" name="password" type="SET" page="shopstartwo" threshold="30000">
				<parameter type="STATIC" value="Halo1234"/>  
			</step>	
		
			<!-- ########### Click Login ############  -->
			<step timed="true" name="Continue.CLICKABLE" page="shopstartwo" type="WAIT_FOR" threshold="30000"  >
				<step timed="true" name="Continue" page="shopstartwo" type="CLICK" threshold="30000" />
			</step>
			
			<step name="elseCondition" type="ELSE" page="Landing_Page" >
				<step timed="true" name="noStoreProperty" page="shopstartwo" type="VISIBLE" threshold="1000" inverse="true" failureMode="FAILURE" />
			</step>
		</step>

		<step type="STATE" name="Test" page="BROWSER" />
	
		<!-- Verify the Width of image -->
				
		<step timed="true" name="ImageWidth.VISIBLE" page="shopstartwo" type="WAIT_FOR" threshold="30000" failureMode="FAILURE"  >
			<step timed="true" name="ImageWidth" page="shopstartwo" type="EXISTS"  />
		</step>
		
		<!-- Verify the Width of Main image -->
				
		<step timed="true" name="ImageTitle.CLICKABLE" page="shopstartwo" type="WAIT_FOR" threshold="30000" failureMode="FAILURE" >
			<step timed="true" name="ImageTitle" page="shopstartwo" type="CLICK" threshold="30000" />
		</step>
		 
		<step timed="true" name="ImageMainSize.VISIBLE" page="shopstartwo" type="WAIT_FOR" threshold="30000" failureMode="FAILURE"  >
			<step timed="true" name="ImageMainSize" page="shopstartwo" type="EXISTS" threshold="30000" />
		</step>
		
		<step timed="true" name="CloseImage.CLICKABLE" page="shopstartwo" type="WAIT_FOR" threshold="30000" failureMode="FAILURE"  >
			<step timed="true" name="CloseImage" page="shopstartwo" type="CLICK" threshold="30000" />
		</step> 
		<step type="STATE" name="Test" page="BROWSER" />
		
		<!-- Get Item code to verify the Item code -->
		<step timed="true" name="ItemCode" type="GET" page="shopstartwo"  threshold="30000" context="Code" />
		
		<step name="STRING" page="shopstartwo" type="COMPARE2" active="true" threshold="30000" timed="true"  wait="0" >
			<parameter type="CONTEXT" name="Value One" value="Code" />
			<parameter type="DATA"  name="Value Two" value="StoreProperty.itemcode" />
			<parameter type="STATIC" name="Operator" value="EQUALS" />
		</step>
		
		<!-- Get Item Title to verify the Item Title -->
		<step timed="true" name="ItemTitle" type="GET" page="shopstartwo"  threshold="30000" context="Title" />

		<step name="STRING" page="shopstartwo" type="COMPARE2" active="true" threshold="30000" timed="true"  wait="0" >
			<parameter type="CONTEXT" name="Value One" value="Title" />
			<parameter type="DATA"  name="Value Two" value="StoreProperty.itemtitle" />
			<parameter type="STATIC" name="Operator" value="EQUALS" />
		</step>

		<!-- ########### Verify Prop 65 ############-->
		<step timed="true" name="prop65Specialnotice.VISIBLE" page="shopstartwo" type="WAIT_FOR" threshold="30000"  >
			<step timed="true" name="prop65Specialnotice" page="shopstartwo" type="EXISTS" threshold="30000" />
		</step>
		
		<!-- Color  -->
		<step timed="true" name="color" type="SET" page="shopstartwo" threshold="30000" failureMode="IGNORE">
			<parameter type="DATA" value="StoreProperty.color"/>  
		</step>
		
		<!-- Size  -->
		<step timed="true" name="size" type="SET" page="shopstartwo" threshold="30000" failureMode="IGNORE">
			<parameter type="DATA" value="StoreProperty.size"/>  
		</step>
		
		<!-- Size  -->
		<step timed="true" name="quantity" type="SET" page="shopstartwo" threshold="30000" failureMode="IGNORE">
			<parameter type="DATA" value="StoreProperty.quantity"/>  
		</step>
		
		<!-- Ink color  -->
		<step timed="true" name="Inkcolor" type="SET" page="shopstartwo" threshold="30000" failureMode="IGNORE">
			<parameter type="DATA" value="StoreProperty.inkcolor"/>  
		</step>
		
		<!-- Lid color  -->
		<step timed="true" name="Lidcolor" type="SET" page="shopstartwo" threshold="30000" failureMode="IGNORE">
			<parameter type="DATA" value="StoreProperty.lidcolor"/>  
		</step>
		
		<step type="STATE" name="Test" page="BROWSER" />
		
		<!-- ########### Click Add to cart ############ 
		<step timed="true" name="addtocart.CLICKABLE" page="shopstartwo" type="WAIT_FOR" threshold="30000"  >
			<step timed="true" name="addtocart" page="shopstartwo" type="CLICK" threshold="30000" />
		</step>-->
		
	
		
		
	</test>	
	
	<test active="false" name="TC_BeMagenta" timed="true" dataDriver="StoreProperty" >

		<step name="URL" type="DEVICE" page="landing_Page" >
			<parameter type="STATIC" value="https://www.mybrandmall.com/preview/facebook/customer-service" />
		</step>
		
		<step type="BROWSER" name="MAXIMIZE" page="Landing_Page" wait="1000" />
		
		<!-- <step timed="true" name="Itemnotunavailable" page="prop65" type="VISIBLE" wait="1000" inverse="true" failureMode="FAILURE" />  -->
		
		<!-- ########### Click ContactUs ############  -->
			<step timed="true" name="ContactUs.CLICKABLE" page="shopstartwo" type="WAIT_FOR" threshold="30000"  >
				<step timed="true" name="ContactUs" page="shopstartwo" type="CLICK" threshold="30000" />
			</step>
			
			
    	<step timed="true" name="Captcha" type="GET" page="shopstartwo" threshold="30000" context="mainCaptcha" >
		</step>
		
		<step type="STATE" name="Test" page="BROWSER" />
		
		<step timed="true" name="mainCaptchatext" type="SET" page="shopstartwo" threshold="30000">
			<parameter type="CONTEXT" value="mainCaptcha"/>
		</step>
		
		<step type="STATE" name="Test" page="BROWSER" />
	
	</test>	
	
	
	
	
	
</suite>	